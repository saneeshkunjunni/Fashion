    <script>
        $(function () {
            'use strict'
            $("[data-utc]").text(function () {
                var utcTime = $(this).attr("data-utc");
                return moment.utc(utcTime, 'DD-MM-YYYY').local().format('DD-MM-YYYY');
            });
            $("[data-utc-time]").text(function () {
                var utcTime = $(this).attr("data-utc-time");
                return moment.utc(utcTime, 'DD-MM-YYYY HH:mm').local().format('DD-MM-YYYY HH:mm');
            });
            $("[data-utc-time-am]").text(function () {
                var utcTime = $(this).attr("data-utc-time-am");
                return moment.utc(utcTime, 'DD-MM-YYYY hh:mm a').local().format('DD-MM-YYYY hh:mm a');
            });
        });
    </script>
